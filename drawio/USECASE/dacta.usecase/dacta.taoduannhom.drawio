        <mxCell id="exceptionflow-row2" style="shape=tableRow;horizontal=0;startSize=0;swimlaneHead=0;swimlaneBody=0;top=0;left=0;bottom=0;right=0;collapsible=0;dropTarget=0;fillColor=none;" parent="table-exceptionflow-teamproject" vertex="1">
          <mxGeometry y="180" width="740" height="110" as="geometry" />
        </mxCell>
        <mxCell id="exceptionflow-r2c1" value="&lt;b&gt;4a. Không có quyền tạo/gán dự án&lt;/b&gt;&lt;br&gt;Người dùng cố gắng tạo hoặc gán dự án mà không có quyền phù hợp." style="shape=partialRectangle;html=1;collapsible=0;dropTarget=0;pointerEvents=0;fillColor=none;top=0;left=0;bottom=0;right=0;strokeColor=inherit;align=left;verticalAlign=top;whiteSpace=wrap;spacingLeft=5;" parent="exceptionflow-row2" vertex="1">
          <mxGeometry width="370" height="110" as="geometry" />
        </mxCell>
        <mxCell id="exceptionflow-r2c2" value="1. Hệ thống hiển thị thông báo 'Không có quyền thực hiện thao tác này'.&lt;br&gt;2. Ngăn chặn thao tác và quay về giao diện trước đó." style="shape=partialRectangle;html=1;collapsible=0;dropTarget=0;pointerEvents=0;fillColor=none;top=0;left=0;bottom=0;right=0;strokeColor=inherit;align=left;verticalAlign=top;whiteSpace=wrap;spacingLeft=5;" parent="exceptionflow-row2" vertex="1">
