        </mxCell>
        <mxCell id="exceptionflow-row3" style="shape=tableRow;horizontal=0;startSize=0;swimlaneHead=0;swimlaneBody=0;top=0;left=0;bottom=0;right=0;collapsible=0;dropTarget=0;fillColor=none;" parent="table-exceptionflow-uploadfile" vertex="1">
          <mxGeometry y="310" width="740" height="120" as="geometry" />
        </mxCell>
        <mxCell id="exceptionflow-r3c1" value="&lt;b&gt;5a. Lỗi lưu trữ file&lt;/b&gt;&lt;br&gt;Hệ thống gặp lỗi khi lưu file vào hệ thống lưu trữ (đầy bộ nhớ, lỗi mạng, v.v.)." style="shape=partialRectangle;html=1;collapsible=0;dropTarget=0;pointerEvents=0;fillColor=none;top=0;left=0;bottom=0;right=0;strokeColor=inherit;align=left;verticalAlign=top;whiteSpace=wrap;spacingLeft=5;" parent="exceptionflow-row3" vertex="1">
          <mxGeometry width="370" height="120" as="geometry" />
        </mxCell>
        <mxCell id="exceptionflow-r3c2" value="1. Hệ thống hiển thị thông báo 'Lỗi khi tải lên file, vui lòng thử lại'.&lt;br&gt;2. Ghi log lỗi để admin kiểm tra.&lt;br&gt;3. Gợi ý người dùng thử lại sau hoặc liên hệ hỗ trợ." style="shape=partialRectangle;html=1;collapsible=0;dropTarget=0;pointerEvents=0;fillColor=none;top=0;left=0;bottom=0;right=0;strokeColor=inherit;align=left;verticalAlign=top;whiteSpace=wrap;spacingLeft=5;" parent="exceptionflow-row3" vertex="1">
