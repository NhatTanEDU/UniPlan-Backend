        </mxCell>
        <mxCell id="exceptionflow-row1" style="shape=tableRow;horizontal=0;startSize=0;swimlaneHead=0;swimlaneBody=0;top=0;left=0;bottom=0;right=0;collapsible=0;dropTarget=0;fillColor=none;" parent="table-exceptionflow-gantttab" vertex="1">
          <mxGeometry y="60" width="740" height="120" as="geometry" />
        </mxCell>
        <mxCell id="exceptionflow-r1c1" value="&lt;b&gt;1a. Không có dữ liệu dự án&lt;/b&gt;&lt;br&gt;Khi dự án không có công việc nào hoặc dữ liệu bị lỗi." style="shape=partialRectangle;html=1;collapsible=0;dropTarget=0;pointerEvents=0;fillColor=none;top=0;left=0;bottom=0;right=0;strokeColor=inherit;align=left;verticalAlign=top;whiteSpace=wrap;spacingLeft=5;" parent="exceptionflow-row1" vertex="1">
          <mxGeometry width="370" height="120" as="geometry" />
        </mxCell>
        <mxCell id="exceptionflow-r1c2" value="1. Hiển thị thông báo &#39;Dự án chưa có công việc nào&#39;.&lt;br&gt;2. Hiển thị biểu đồ Gantt trống với khung thời gian.&lt;br&gt;3. Gợi ý tạo công việc mới hoặc quay về trang quản lý dự án." style="shape=partialRectangle;html=1;collapsible=0;dropTarget=0;pointerEvents=0;fillColor=none;top=0;left=0;bottom=0;right=0;strokeColor=inherit;align=left;verticalAlign=top;whiteSpace=wrap;spacingLeft=5;" parent="exceptionflow-row1" vertex="1">
          <mxGeometry x="370" width="370" height="120" as="geometry" />
        </mxCell>
        <mxCell id="exceptionflow-row2" style="shape=tableRow;horizontal=0;startSize=0;swimlaneHead=0;swimlaneBody=0;top=0;left=0;bottom=0;right=0;collapsible=0;dropTarget=0;fillColor=none;" parent="table-exceptionflow-gantttab" vertex="1">
          <mxGeometry y="180" width="740" height="130" as="geometry" />
        </mxCell>
        <mxCell id="exceptionflow-r2c1" value="&lt;b&gt;3a. Thời gian không hợp lệ&lt;/b&gt;&lt;br&gt;Khi thay đổi thời gian công việc bằng kéo-thả mà thời gian không hợp lệ (ngày bắt đầu sau ngày kết thúc, xung đột với công việc khác, v.v.)." style="shape=partialRectangle;html=1;collapsible=0;dropTarget=0;pointerEvents=0;fillColor=none;top=0;left=0;bottom=0;right=0;strokeColor=inherit;align=left;verticalAlign=top;whiteSpace=wrap;spacingLeft=5;" parent="exceptionflow-row2" vertex="1">
          <mxGeometry width="370" height="130" as="geometry" />
        </mxCell>
        <mxCell id="exceptionflow-r2c2" value="1. Hệ thống hiển thị thông báo lỗi cụ thể về thời gian không hợp lệ.&lt;br&gt;2. Khôi phục vị trí ban đầu của thanh công việc.&lt;br&gt;3. Yêu cầu người dùng chỉnh sửa lại thời gian phù hợp.&lt;br&gt;4. Quay lại bước 3 của luồng chính." style="shape=partialRectangle;html=1;collapsible=0;dropTarget=0;pointerEvents=0;fillColor=none;top=0;left=0;bottom=0;right=0;strokeColor=inherit;align=left;verticalAlign=top;whiteSpace=wrap;spacingLeft=5;" parent="exceptionflow-row2" vertex="1">
          <mxGeometry x="370" width="370" height="130" as="geometry" />
        </mxCell>
        <mxCell id="exceptionflow-row3" style="shape=tableRow;horizontal=0;startSize=0;swimlaneHead=0;swimlaneBody=0;top=0;left=0;bottom=0;right=0;collapsible=0;dropTarget=0;fillColor=none;" parent="table-exceptionflow-gantttab" vertex="1">
          <mxGeometry y="310" width="740" height="120" as="geometry" />
        </mxCell>
        <mxCell id="exceptionflow-r3c1" value="&lt;b&gt;3b. Phụ thuộc không hợp lệ&lt;/b&gt;&lt;br&gt;Khi tạo liên kết phụ thuộc giữa các công việc mà tạo ra vòng lặp hoặc xung đột logic." style="shape=partialRectangle;html=1;collapsible=0;dropTarget=0;pointerEvents=0;fillColor=none;top=0;left=0;bottom=0;right=0;strokeColor=inherit;align=left;verticalAlign=top;whiteSpace=wrap;spacingLeft=5;" parent="exceptionflow-row3" vertex="1">
          <mxGeometry width="370" height="120" as="geometry" />
        </mxCell>
        <mxCell id="exceptionflow-r3c2" value="1. Hệ thống hiển thị thông báo &#39;Không thể tạo phụ thuộc - tạo vòng lặp&#39;.&lt;br&gt;2. Hủy bỏ thao tác tạo liên kết.&lt;br&gt;3. Giữ nguyên trạng thái hiện tại của biểu đồ." style="shape=partialRectangle;html=1;collapsible=0;dropTarget=0;pointerEvents=0;fillColor=none;top=0;left=0;bottom=0;right=0;strokeColor=inherit;align=left;verticalAlign=top;whiteSpace=wrap;spacingLeft=5;" parent="exceptionflow-row3" vertex="1">
          <mxGeometry x="370" width="370" height="120" as="geometry" />
        </mxCell>
        <mxCell id="exceptionflow-row4" style="shape=tableRow;horizontal=0;startSize=0;swimlaneHead=0;swimlaneBody=0;top=0;left=0;bottom=0;right=0;collapsible=0;dropTarget=0;fillColor=none;" parent="table-exceptionflow-gantttab" vertex="1">
          <mxGeometry y="430" width="740" height="60" as="geometry" />
        </mxCell>
        <mxCell id="exceptionflow-r4c1" value="&lt;b&gt;4a. Lỗi lưu thay đổi&lt;/b&gt;&lt;br&gt;Hệ thống gặp lỗi khi lưu thay đổi vào CSDL." style="shape=partialRectangle;html=1;collapsible=0;dropTarget=0;pointerEvents=0;fillColor=none;top=0;left=0;bottom=0;right=0;strokeColor=inherit;align=left;verticalAlign=top;whiteSpace=wrap;spacingLeft=5;" parent="exceptionflow-row4" vertex="1">
